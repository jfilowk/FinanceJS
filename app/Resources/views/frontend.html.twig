{% extends '::base.html.twig' %}

{% block stylesheets %}

    <link href="{{ asset('bundles/transaction/css/bootstrap.min.css') }}" rel="stylesheet" type="-text/css">
    <link href="{{ asset('bundles/transaction/css/custom.css') }}" rel="stylesheet" type="-text/css">
    <link href="{{ asset('bundles/transaction/css/bootstrap-theme.css') }}" rel="stylesheet" type="-text/css">

{% endblock %}



{% block body %}
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar"
                        aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Project name</a>
            </div>
            <div id="navbar" class="collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    <li class="active"><a href="#">Home</a></li>
                    <li><a href="#about">About</a></li>
                    <li><a href="#contact">Contact</a></li>
                </ul>
            </div>
            <!--/.nav-collapse -->
        </div>
    </nav>

    <div class="container">
        <div class="col-xs-9 col-sm-9 col-md-9 col-lg-9" style="background-color: #0088CC">
            <h1>Datos</h1>
        	{% block main %}
            {% endblock %}
        </div>
        <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3" style="background-color: #080808">
            <h1>Extra</h1>
        	{% block extra %}
            {% endblock %}
        </div>
    </div>

{% endblock %}

{% block javascripts %}
    <script src="{{ asset('bundles/transaction/js/jquery-1.11.2.js') }}" ></script>
    <script src="{{ asset('bundles/transaction/js/bootstrap.min.js') }}" ></script>
    <script src="{{ asset('bundles/transaction/js/custom.js') }}" ></script>
{% endblock %}